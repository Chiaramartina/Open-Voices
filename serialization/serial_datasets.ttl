    dcat3:releaseDate "2024"^^xsd:year ;
    dcat3:theme <http://publications.europa.eu/resource/authority/data-theme/SOCI> ;
    dcat3:title "Females Victim Rates per 100k Inhabitants by Region in Italy"@en .

ov:MD8_VICTIMS_GEO_MALES a prov:Entity,
        dcat3:Dataset ;
    dct:accessRights "Public"@en ;
    dct:description "The dataset details male victim rates per 100,000 inhabitants across Italian regions. Lazio reports the highest rate (0.64%), followed by Basilicata, Abruzzo, and Puglia (0.53–0.54%). Calabria has the lowest rate (0.16%), while most regions fall between 0.3 and 0.4%."@en ;
    prov:wasAttributedTo <https://github.com/Asemica-me>,
